{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "warn"
    ],
    "no-prototype-builtins": "off",
    "no-plusplus": "off",
    "react/jsx-filename-extension": "off"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  }
}